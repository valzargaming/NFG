{
  "name": "note-form-generator",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "lint": "echo \"No linter configured\"",
    "cs-fixer": "prettier --write \"**/*.{js,html,json,css,md}\" && node -e \"require('child_process').spawnSync('eslint',['--fix','**/*.js'],{stdio:'inherit',shell:true})\"",
    "cs-fixer:prettier": "prettier --write \"**/*.{js,html,json,css,md}\"",
    "cs-fixer:eslint": "node -e \"require('child_process').spawnSync('eslint',['--fix','**/*.js'],{stdio:'inherit',shell:true})\""
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testTimeout": 10000,
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "devDependencies": {
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^29.6.0",
    "jest-environment-jsdom": "^29.6.0",
    "jsdom": "^22.1.0",
    "prettier": "^2.8.8"
  }
}
